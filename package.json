{
  "name": "svg-sprite-empower",
  "version": "2.3.0",
  "description": "This Figma plugin makes it easy to create svg sprites based on selection",
  "author": "Profesor08",
  "figma-plugin": {
    "name": "Svg Sprite Empower",
    "id": "1210268390758084662",
    "editorType": [
      "figma",
      "dev"
    ],
    "capabilities": [
      "panel"
    ],
    "main": "src/plugin/plugin.ts",
    "ui": "src/ui/ui.tsx"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "dev": "build-figma-plugin --typecheck --watch",
    "typecheck": "npx tsc --noEmit --skipLibCheck true",
    "lint": "npx eslint ."
  },
  "dependencies": {
    "@create-figma-plugin/ui": "^2.6.1",
    "@create-figma-plugin/utilities": "^2.6.1",
    "clsx": "^2.0.0",
    "highlight.js": "^11.8.0",
    "lodash": "^4.17.21",
    "preact": "^10.17.1",
    "pretty": "^2.0.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^2.6.1",
    "@figma/plugin-typings": "^1.76.0",
    "@types/lodash": "^4.14.197",
    "@types/pretty": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "typescript": "^5.2.2"
  }
}
