{
  "name": "svg-sprite-empower",
  "version": "3.0.0",
  "description": "This Figma plugin makes it easy to create svg sprites based on selection",
  "author": "Profesor08",
  "type": "module",
  "scripts": {
    "lint": "npx eslint .",
    "dev": "concurrently -n ui,plugin \"npm:dev:ui -- --watch\" \"npm:dev:plugin -- --watch\"",
    "dev:ui": "npx vite build --emptyOutDir=false --mode=development",
    "dev:plugin": "esbuild src/plugin/plugin.ts --outfile=dist/plugin.js --bundle --target=es6",
    "build": "npm run build:ui && npm run build:plugin",
    "build:ui": "npx vite build --emptyOutDir=false",
    "build:plugin": "esbuild src/plugin/plugin.ts --outfile=dist/plugin.js --bundle --target=es6 --minify"
  },
  "dependencies": {
    "@figma/plugin-typings": "^1.83.0",
    "@types/lodash": "^4.14.202",
    "@types/pretty": "^2.0.3",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "antd": "^5.12.8",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.11",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "prettier": "^3.1.1",
    "pretty": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "sass": "^1.69.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-tsconfig-paths": "^4.2.3",
    "zustand": "^4.4.7"
  }
}
